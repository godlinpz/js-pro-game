{
  "name": "js-pro-game",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/godlinpz/js-pro-game.git",
  "author": "Dmitry Pavlichenko <godlin.pz@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.5.1"
  },
  "scripts": {
    "build:dev": "env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "env NODE_ENV=production webpack --config webpack.config.js",
    "start1": "env NODE_ENV=development webpack-dev-server",
    "start": "env NODE_ENV=development webpack serve",
    "lint": "yarn eslint -c eslintrc.js ./src"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
        "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "yarn pretty-quick --staged",
      "yarn lint"
    ]
  }
}
